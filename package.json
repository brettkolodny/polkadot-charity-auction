{
  "name": "polkadot-charity-raffle",
  "version": "0.1.0",
  "scripts": {
    "re:build": "bsb -make-world",
    "re:start": "bsb -make-world -w -ws _ ",
    "re:clean": "bsb -clean-world",
    "server": "parcel serve index.html",
    "build": "yarn re:build; parcel build index.html --public-url ./",
    "test": "echo \"Error: no test specified\" && exit 1",
    "setup": "canvas --dev --tmp &>/dev/null & node ./setup.js; yarn re:build; yarn server"
  },
  "keywords": [
    "BuckleScript",
    "ReasonReact",
    "reason-react"
  ],
  "author": "brettkolodny",
  "license": "MIT",
  "devDependencies": {
    "bs-platform": "8.2.0",
    "parcel-bundler": "^1.12.4"
  },
  "dependencies": {
    "@polkadot/api": "^2.3.1",
    "@polkadot/api-contract": "^2.3.1",
    "@polkadot/extension-dapp": "^0.34.1",
    "@polkadot/keyring": "^3.6.1",
    "@polkadot/react-identicon": "^0.61.1",
    "@polkadot/util": "^3.6.1",
    "@polkadot/util-crypto": "^3.6.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-is": "^17.0.1",
    "reason-react": "0.9.1",
    "styled-components": "^5.2.0"
  }
}
